package hr.fina.regzap.hr.spring.rest.model;


public class Error {
	
	private String iErrorId;
	private String iErrorMsg;
	
	public Error(String pErrorId, String pErrorMsg) {
		this.iErrorId = pErrorId;
		this.iErrorMsg =pErrorMsg ;
	}

	public String getErrorId() {
		return iErrorId;
	}

	public void setErrorId(String pErrorId) {
		this.iErrorId = pErrorId;
	}

	public String getErrorMsg() {
		return iErrorMsg;
	}

	public void setErrorMsg(String pErrorMsg) {
		this.iErrorMsg = pErrorMsg;
	}
}
